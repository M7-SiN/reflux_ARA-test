    "@nestjs/schedule": "^5.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "cheerio": "^1.0.0",
    "damerau-levenshtein": "^1.0.8",
    "he": "^1.2.0",
    "nestjs-zod": "^4.3.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "sanitize-html": "^2.15.0",
    "string-similarity": "^4.0.4",
